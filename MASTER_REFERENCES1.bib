	issn = {1556-6021},
	journal = {IEEE Transactions on Information Forensics and Security},
	keywords = {asynchronous botnet, Botnet, Brute-force attack, Computational modeling, Games, guesswork, IP networks, Password, Synchronization, targeted password guessing},
	month = sep,
	note = {Conference Name: IEEE Transactions on Information Forensics and Security},
	number = {9},
	pages = {2288--2299},
	shorttitle = {Why {Botnets} {Work}},
	title = {Why Botnets {Work}: Distributed {Brute}-{Force} Attacks Need No Synchronization},
