	doi = {10.1145/857076.857078},
	file = {Submitted Version:files/1977/Eugster et al. - 2003 - The many faces of publishsubscribe.pdf:application/pdf},
	issn = {0360-0300},
	journal = {ACM Comput. Surv.},
	keywords = {Distribution, interaction, publish/subscribe},
	month = jun,
	number = {2},
	pages = {114--131},
	title = {The many faces of publish/subscribe},
