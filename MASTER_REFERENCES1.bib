	file = {arXiv Fulltext PDF:/Users/paulborrill/Zotero/storage/P6FUK6P4/Gogioso and Pinzani - 2023 - The Combinatorics of Causality.pdf:application/pdf;arXiv.org Snapshot:/Users/paulborrill/Zotero/storage/4VZ7FH78/2206.html:text/html},
	keywords = {Quantum Physics},
	month = jul,
	note = {arXiv:2206.08911 [quant-ph]},
