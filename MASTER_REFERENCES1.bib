	keywords = {Data compression, Decoding, Entropy, Information rates, Laboratories, Random variables, Rate distortion theory, Rate-distortion, Relays, Source coding},
	month = oct,
	note = {ISSN: 1058-6393},
	pages = {2137--2141},
