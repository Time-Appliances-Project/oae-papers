	file = {arXiv Fulltext PDF:files/103/Marshall et al. - 2019 - Quantifying the pathways to life using assembly sp.pdf:application/pdf;arXiv.org Snapshot:files/104/1907.html:text/html},
	keywords = {Computer Science - Artificial Intelligence, Physics - Biological Physics},
	month = aug,
	note = {arXiv:1907.04649 [physics]},
