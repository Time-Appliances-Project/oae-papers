	keywords = {CODIERUNG (INFORMATIONSTHEORIE), CODING (INFORMATION THEORY), Electric engineering, FOS: Mathematics, info:eu-repo/classification/ddc/510, info:eu-repo/classification/ddc/621.3, Mathematics, SIGNAL TRANSMISSION + DATA COMMUNICATION (TELECOMMUNICATIONS), SIGNAL{\ U}BERTRAGUNG + DATENKOMMUNIKATION (NACHRICHTENTECHNIK)},
	language = {en},
	note = {Medium: application/pdf,Online-Ressource Publisher: ETH Zurich},
