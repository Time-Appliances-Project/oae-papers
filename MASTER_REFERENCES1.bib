	issn = {1557-9654},
	journal = {IEEE Transactions on Information Theory},
	keywords = {Computer architecture, Data compression, Data privacy, Databases, Decoding, Distributed computing, distributed source coding, functional compression, Laboratories, Source coding, Statistics, Temperature sensors, Wireless sensor networks},
	month = aug,
