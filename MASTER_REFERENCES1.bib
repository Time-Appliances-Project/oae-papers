	keywords = {Quantum Physics},
	month = {3},
	note = {arXiv:2203.11245 [quant-ph]},
	publisher = {arXiv},
	shorttitle = {Embedding cyclic causal structures in acyclic spacetimes},
	title = {Embedding cyclic causal structures in acyclic spacetimes: no-go results for process matrices},
	url = {http://arxiv.org/abs/2203.11245},
	urldate = {2023-01-12},
