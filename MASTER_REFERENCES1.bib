	issn = {1558-2566},
	journal = {IEEE/ACM Transactions on Networking},
	keywords = {Broadcast, Decoding, delay, Delays, Encoding, in-order packet delivery, Markov processes, network coding, Network coding, rate control, Receivers, throughput, Throughput},
	month = dec,
