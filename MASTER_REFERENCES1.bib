	issn = {1099-4300},
	journal = {Entropy},
	keywords = {Quantum Physics, Computer Science - Emerging Technologies, Condensed Matter - Statistical Mechanics},
	month = jun,
	note = {arXiv:2105.00065 [cond-mat, physics:quant-ph]},
