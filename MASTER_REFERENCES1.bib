	keywords = {Condensed Matter - Statistical Mechanics, Mathematics - Category Theory, Quantum Physics},
	month = feb,
	note = {arXiv:2302.07280 [cond-mat, physics:quant-ph]},
	publisher = {arXiv},
	shorttitle = {From dual-unitary to biunitary},
	title = {From dual-unitary to biunitary: a 2-categorical model for exactly-solvable many-body quantum dynamics},
