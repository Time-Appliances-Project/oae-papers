	journal = {IEEE Transactions on Communications},
	keywords = {BCH, burst errors, Codes, GRAND, hard detection decoders, interleaver, Markov processes, Maximum likelihood decoding, Memoryless systems, Modulation, QAM, Quadrature amplitude modulation, random linear codes, Reed-Muller, short codes, Symbols, URLLC},
