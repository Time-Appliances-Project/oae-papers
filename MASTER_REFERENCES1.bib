@misc{Xu2022,
	abstract = {The fundamental nature of Dark Matter remains one of the major mysteries of modern physics. Some models postulate the existence of a massive gauge boson, a  dark photon  (\$A{\textasciicircum}{\textbackslash}prime\$), that may allow Dark Matter particles to interact with Standard Model particles. Ultra-relativistic heavy-ion collisions produce highly Lorentz-contracted electromagnetic fields with sufficient energy density to potentially manifest as light dark photons. We report limits on dark photon parameters via a search for anomalous production of \$e{\textasciicircum}+e{\textasciicircum}-\$ pairs in \${\textbackslash}gamma A{\textasciicircum}{\textbackslash}prime\$ and \$A{\textasciicircum}{\textbackslash}prime A{\textasciicircum}{\textbackslash}prime\$ fusion from ultra-peripheral \${\textbackslash}rm\{Au\}+{\textbackslash}rm\{Au\}\$ collisions. This study utilizes measurements of the Breit-Wheeler process (\${\textbackslash}gamma{\textbackslash}gamma {\textbackslash}rightarrow e{\textasciicircum}+e{\textasciicircum}-\$) carried out by STAR, specifically making use of polarization-dependent final-state azimuthal asymmetries in \$e{\textasciicircum}+e{\textasciicircum}-\$ pairs. These limits are informative for future searches in the ultra-peripheral heavy-ion collisions and to constrain future theoretical developments of the dark photon mechanism.},
	annote = {Comment: 6 pages, 3 figures},
	author = {Xu, Isabel and Lewis, Nicole and Wang, Xiaofeng and Brandenburg, James Daniel and Ruan, Lijuan},
