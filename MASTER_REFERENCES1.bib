	file = {ScienceDirect Snapshot:files/1026/S1872497316300254.html:text/html},
	issn = {1872-4973},
	journal = {Forensic Science International: Genetics},
	keywords = {-Value, DNA analysis, Likelihood ratio, LR distribution, Mixture interpretation},
	language = {en},
