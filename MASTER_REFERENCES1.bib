	keywords = {concurrency, determinism, bisimulation, model checking, time Petri nets, Time-critical systems},
	month = may,
	number = {2},
	pages = {11:1--11:27},
	title = {Time-{Soundness} of Time Petri Nets Modelling {Time}-{Critical} Systems},
	url = {https://doi.org/10.1145/3185502},
