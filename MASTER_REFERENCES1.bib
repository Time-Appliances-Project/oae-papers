	keywords = {Mathematics - Category Theory, Quantum Physics},
	month = {2},
	note = {arXiv:1501.06489 [quant-ph]},
	publisher = {arXiv},
	title = {Categorical Semantics for {Schr}{\textbackslash} odinger's Equation},
	url = {http://arxiv.org/abs/1501.06489},
	urldate = {2023-01-12},
