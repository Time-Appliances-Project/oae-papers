	keywords = {AWGN, Bandwidth, Channel state information, Channel state information (CSI), coherence level, energy efficiency, Energy efficiency, Fading, Network address translation, noncoherent, Rayleigh channels, Signal to noise ratio, Ultra wideband communication, wideband communication},
