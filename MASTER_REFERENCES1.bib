	issn = {1558-2248},
	journal = {IEEE Transactions on Wireless Communications},
	keywords = {Additive white noise, AWGN channels, Broadcasting, Decoding, Degradation, Design optimization, Laboratories, Process design, Traffic control},
	month = mar,
	note = {Conference Name: IEEE Transactions on Wireless Communications},
	number = {2},
