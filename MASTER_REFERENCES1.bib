	keywords = {High Energy Physics - Experiment},
	month = nov,
	note = {arXiv:2211.02132 [hep-ex]},
	publisher = {arXiv},
	title = {Search for Dark Photons in \${\textbackslash}gamma{\textbackslash}gamma{\textbackslash}rightarrow e{\textasciicircum}+e{\textasciicircum}-\$ at RHIC},
