	issn = {1557-9654},
	journal = {IEEE Transactions on Information Theory},
	keywords = {Block codes, Channel coding, communication systems, data compression, Data compression, decoding, Encoding, Iterative algorithms, Iterative decoding, iterative methods, Laboratories, Linear code, Linear programming, Maximum likelihood decoding, Signal processing algorithms},
	month = aug,
