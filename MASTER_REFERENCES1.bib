	keywords = {Amplify-and-forward, capacity, Diamond-like carbon, high SNR regime, Network coding, relaying, Relays, Signal to noise ratio, Spread spectrum communication, Wireless networks},
	month = feb,
	note = {Conference Name: IEEE Transactions on Information Theory},
	number = {2},
	pages = {793--803},
	shorttitle = {Multihop {Analog} {Network} {Coding} via {Amplify}-and-{Forward}},
