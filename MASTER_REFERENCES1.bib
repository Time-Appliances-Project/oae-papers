	file = {IEEE Xplore Abstract Record:files/2067/9864037.html:text/html},
	issn = {1557-9956},
	journal = {IEEE Transactions on Computers},
	keywords = {Behavioral sciences, common-case specialization, Costs, data dependent, Field programmable gate arrays, FPGA acceleration, IP networks, Pattern matching, Runtime, stream processing, Streaming media},
	note = {Conference Name: IEEE Transactions on Computers},
	pages = {1--13},
	title = {Exploiting the Common Case when Accelerating {Input}-{Dependent} Stream Processing by FPGA},
	year = {2022},
