	keywords = {Computer Science - Machine Learning, Mathematics - Category Theory, Quantum Physics, Statistics - Machine Learning},
	month = apr,
	note = {arXiv:2004.05631 [quant-ph, stat]},
	publisher = {arXiv},
	title = {At the Interface of Algebra and Statistics},
