	file = {IEEE Xplore Abstract Record:files/880/1638540.html:text/html;Submitted Version:files/879/Deb et al. - 2006 - Algebraic gossip a network coding approach to opt.pdf:application/pdf},
	journal = {IEEE Transactions on Information Theory},
	keywords = {Centralized control, Communication system control, Distributed algorithms, Distributed computing, Electrical equipment industry, Gossip algorithms, Laboratories, message dissemination, network coding, Network coding, Protocols, Time measurement, Time varying systems},
	month = jun,
	note = {Conference Name: IEEE Transactions on Information Theory},
	number = {6},
