	file = {IEEE Xplore Abstract Record:files/1399/1355980.html:text/html},
	issn = {1558-0008},
	journal = {IEEE Journal on Selected Areas in Communications},
	keywords = {Aerospace control, Aircraft, Communication system control, Link failures, Local area networks, network design, network reliability, Network topology, Optical fiber LAN, Optical fiber networks, physical network topology, Redundancy, Stress, Telecommunication network reliability},
	month = nov,
	note = {Conference Name: IEEE Journal on Selected Areas in Communications},
	number = {9},
