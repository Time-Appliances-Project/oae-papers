	keywords = {Bandwidth, Delay, Frequency, Gaussian noise, Linear code, Network coding, Propagation losses, Routing protocols, Space technology, Underwater Acoustic Networks, Network Coding, Lower Bound for transmission power, minimal transmission power, bandwidth - distance dependence, Underwater acoustics},
	month = dec,
