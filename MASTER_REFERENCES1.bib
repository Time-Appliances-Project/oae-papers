	journal = {IEEE Transactions on Information Theory},
	keywords = {Ad hoc networks, Broadcasting, communication networks, Communication networks, Computer networks, distributed algorithms, dynamic multicast groups, Dynamic programming, Laboratories, multicast, Multicast algorithms, network coding, Network coding, network optimization, Polynomials, Unicast, wireless networks, Wireless networks},
