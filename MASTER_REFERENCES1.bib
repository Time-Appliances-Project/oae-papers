	issn = {1557-9654},
	journal = {IEEE Transactions on Information Theory},
	keywords = {approximate ML decoding, Approximation algorithms, channel coding, Channel coding, Complexity theory, Discrete channels, error probability, Manganese, maximum likelihood decoding, Maximum likelihood decoding, Receivers},
