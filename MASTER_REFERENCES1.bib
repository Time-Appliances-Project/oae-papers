	issn = {1558-0857},
	journal = {IEEE Transactions on Communications},
	keywords = {Codes, Complexity theory, Linear codes, Maximum likelihood decoding, Phase shift keying, Quantization (signal), random codes, Reliability, symbol reliability information, Universal decoding},
	month = jan,
	note = {Conference Name: IEEE Transactions on Communications},
	number = {1},
