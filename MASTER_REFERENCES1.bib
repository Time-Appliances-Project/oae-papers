	issn = {1367-2630},
	journal = {New Journal of Physics},
	month = mar,
	number = {3},
	pages = {033049},
	title = {Stochastic thermodynamics and fluctuation theorems for non-linear systems},
	url = {https://iopscience.iop.org/article/10.1088/1367-2630/abea46},
	urldate = {2023-01-16},
