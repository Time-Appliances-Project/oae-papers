	keywords = {Adaptive systems, Broadcast approach, Communication channels, Delays, Encoding, Forward error correction, in-order delivery delay, network coding, Network coding, Receivers, throughput, ultra-reliable low-latency},
	month = jun,
	note = {ISSN: 2157-8117},
