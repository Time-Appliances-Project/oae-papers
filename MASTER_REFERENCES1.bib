	file = {Full Text PDF:files/1961/Liu et al. - 2021 - When threads meet events efficient and precise st.pdf:application/pdf},
	isbn = {978-1-4503-8391-2},
	keywords = {Data Race Detection, Origins, Pointer Analysis, Static Analysis},
	month = jun,
	pages = {725--739},
