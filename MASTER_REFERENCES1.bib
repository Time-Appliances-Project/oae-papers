	keywords = {Decoding, Delay, Indexes, Markov processes, Network coding, Nickel, Receivers},
	month = dec,
	note = {ISSN: 1930-529X},
	pages = {1--6},
	title = {Online Network Coding for {Time}-{Division} Duplexing},
