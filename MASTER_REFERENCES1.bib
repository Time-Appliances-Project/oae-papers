@book{Wood2022,
	abstract = {This document describes a correspondence between photons and propagating information packets (PIPs) that are emergent out of the Space Element Reduction Duplication (SERD) model. The SERD model is a discrete background independent microscopic space-time description. By observation of physically comparable behaviour emerging from this system, through analysis and computer simulation, we draw conclusions of what the form and dynamics of the true underlying space-time may be. By treating elements of the system as fundamental observers, mathematical and empirical evidence is obtained of the existence of fully emergent light-like cosmological horizons, implying the existance of causally separated pocket universes. The SERD model is a network of connected hyperedges called Point Particles (PPs) which represent the fundamental constituents of all matter and particles (and therefore observers) separated by strings of consecutive and fundamental elements called Space Elements (SE). The SEs duplicate and reduce while the PPs split and merge through discrete time. These are known as the actions of the elements and create a highly dynamic multi-way system. Elements have an awareness of the information around them and communicate with their nearest neighbours through time.},
	author = {Wood, Thomas},
	date-added = {2023-05-22 00:30:22 -0700},
	date-modified = {2023-05-22 00:30:22 -0700},
