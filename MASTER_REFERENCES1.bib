	doi = {10.48550/arXiv.2305.14270},
	file = {arXiv Fulltext PDF:files/510/Lu et al. - 2023 - NCC Natural Concurrency Control for Strictly Seri.pdf:application/pdf;arXiv.org Snapshot:files/511/2305.html:text/html},
	keywords = {C.2.4, Computer Science - Distributed, Parallel, and Cluster Computing},
	month = may,
	note = {arXiv:2305.14270 [cs]},
	publisher = {arXiv},
	shorttitle = {{NCC}},
	title = {{NCC}: Natural Concurrency Control for Strictly Serializable Datastores by Avoiding the {Timestamp}-{Inversion} Pitfall},
	url = {http://arxiv.org/abs/2305.14270},
	urldate = {2023-05-27},
