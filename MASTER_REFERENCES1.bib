	issn = {1557-9654},
	journal = {IEEE Transactions on Information Theory},
	keywords = {Aggregates, Correlation, Data privacy, Estimation, Measurement, minimum mean-squared error, Motion pictures, Mutual information, Privacy, privacy-utility trade-off},
	month = dec,
