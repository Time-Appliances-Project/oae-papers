	language = {en},
	month = nov,
	pages = {1307--1322},
	publisher = {ACM},
	shorttitle = {{DangZero}},
	title = {{DangZero}: Efficient {Use}-{After}-{Free} Detection via Direct Page Table Access},
	url = {https://dl.acm.org/doi/10.1145/3548606.3560625},
	urldate = {2023-01-03},
