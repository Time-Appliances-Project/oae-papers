	issn = {1557-9654},
	journal = {IEEE Transactions on Information Theory},
	keywords = {Communication networks, Graph theory, Information management, Laboratories, network coding, Network coding, network management, network restoration, Protection, Robustness, Routing, Shannon theory, Switches, Telecommunication traffic, Upper bound},
	month = {4},
	note = {Conference Name: IEEE Transactions on Information Theory},
