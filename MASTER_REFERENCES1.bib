	journal = {IEEE Transactions on Information Theory},
	keywords = {AWGN, Bandwidth, Broadband antennas, Energy efficiency, Error probability, Information theory, low signal-to-noise ratio (SNR), MIMO, multiple-input multiple-output (MIMO), noncoherent, Rayleigh channels, Receiving antennas, Signal to noise ratio, Wideband, wideband channels},
	month = jun,
	note = {Conference Name: IEEE Transactions on Information Theory},
	number = {6},
