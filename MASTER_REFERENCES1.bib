	file = {Full Text PDF:files/332/Gordon and Stout - 1988 - Hypercube message routing in the presence of fault.pdf:application/pdf},
	isbn = {978-0-89791-278-5},
	month = {1},
	pages = {318--327},
	publisher = {Association for Computing Machinery},
	series = {C$^{\textrm{3}}${P}},
	title = {Hypercube message routing in the presence of faults},
	url = {https://doi.org/10.1145/62297.62332},
