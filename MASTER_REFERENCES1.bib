	journal = {IEEE Transactions on Communications},
	keywords = {Adaptation models, adaptive, causal, coding, Decoding, Delays, Encoding, feedback, Forward error correction, forward error correction (FEC), in order delivery delay, Network coding, Random linear network coding (RLNC), throughput, Throughput},
	month = jul,
