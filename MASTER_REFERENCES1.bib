	keywords = {Block codes, Channel coding, Data compression, Iterative algorithms, Iterative decoding, Laboratories, Linear programming, Maximum likelihood decoding, Parity check codes, Technological innovation},
	month = mar,
	note = {ISSN: 1068-0314},
	pages = {282--291},
	title = {On some new approaches to practical {Slepian}-{Wolf} compression inspired by channel coding},
