@article{Marletto2022,
	abstract = {The issue of irreversibility in a universe with time-reversal-symmetric laws is a central problem in physics. , and, in particular, to statistical mechanics, information theory and quantum thermodynamics. In this letter, we discuss for the first time how irreversibility can emerge within the recently proposed constructor theory framework. Here irreversibility is expressed as the requirement that a task is possible, while its inverse is not. In particular, we demonstrate that this irreversibility is compatible with quantum theory's time reversal symmetric laws, by exploiting a specific model, based on the universal quantum homogeniser, realised experimentally with high-quality single-photon qubits.},
	author = {Marletto, Chiara and Vedral, Vlatko and Knoll, Laura and Piacentini, Fabrizio and Bernardi, Ettore and Rebufello, Enrico and Avella, Alessio and Gramegna, Marco and Degiovanni, Ivo Pietro and Genovese, Marco},
