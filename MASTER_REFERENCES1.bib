	keywords = {Computer Science - Hardware Architecture},
	month = jan,
	note = {arXiv:2301.04767 [cs]},
	publisher = {arXiv},
	shorttitle = {{RAD}-{Sim}},
	title = {{RAD}-{Sim}: Rapid Architecture Exploration for Novel Reconfigurable Acceleration Devices},
	url = {http://arxiv.org/abs/2301.04767},
