	journal = {Journal of Physics A: Mathematical and Theoretical},
	language = {en},
	month = mar,
	number = {12},
	pages = {3081},
	shorttitle = {Towards quantum gravity},
	title = {Towards quantum gravity: a framework for probabilistic theories with non-fixed causal structure},
	url = {https://dx.doi.org/10.1088/1751-8113/40/12/S12},
	urldate = {2023-01-15},
	volume = {40},
