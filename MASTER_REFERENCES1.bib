	isbn = {978-1-66549-726-8},
	language = {en},
	month = jul,
	pages = {1--10},
	publisher = {IEEE},
	shorttitle = {Exon},
	title = {Exon: An Oblivious {Exactly}-{Once} Messaging Protocol},
	url = {https://ieeexplore.ieee.org/document/9868879/},
	urldate = {2022-12-28},
