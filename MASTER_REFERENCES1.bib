	doi = {10.1109/DCC.2007.34},
	file = {IEEE Xplore Abstract Record:files/1276/4148748.html:text/html;Submitted Version:files/1275/Doshi et al. - 2007 - Distributed Functional Compression through Graph C.pdf:application/pdf},
	keywords = {Computational modeling, Data compression, Decoding, Distributed computing, Encoding, Entropy, Laboratories, Relays, Sensor phenomena and characterization, Source coding},
	month = mar,
	note = {ISSN: 1068-0314},
	pages = {93--102},
	title = {Distributed Functional Compression through Graph Coloring},
	year = {2007},
