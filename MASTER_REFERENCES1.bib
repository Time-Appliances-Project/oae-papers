	issn = {1558-0008},
	journal = {IEEE Journal on Selected Areas in Communications},
	keywords = {Bandwidth, Byzantine, Contamination, content distribution, Costs, distributed storage, Laboratories, Network coding, peer to peer, Peer to peer computing, Robustness, Secure storage, security, Telecommunications, Throughput},
	month = jun,
