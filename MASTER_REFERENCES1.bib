	doi = {10.48550/arXiv.2207.09555},
	file = {arXiv Fulltext PDF:files/208/Bateni et al. - 2022 - Xronos Predictable Coordination for Safety-Critic.pdf:application/pdf;arXiv.org Snapshot:files/209/2207.html:text/html},
	keywords = {Computer Science - Distributed, Parallel, and Cluster Computing},
	month = jul,
	note = {arXiv:2207.09555 [cs]},
	publisher = {arXiv},
	shorttitle = {Xronos},
	title = {Xronos: Predictable Coordination for {Safety}-{Critical} Distributed Embedded Systems},
	url = {http://arxiv.org/abs/2207.09555},
	urldate = {2023-01-29},
	year = {2022},
