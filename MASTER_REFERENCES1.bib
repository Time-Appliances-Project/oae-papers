	keywords = {Bridges, Decoding, Feedback, Laboratories, Network coding, Stability, Stochastic processes, Throughput, Transmitters},
	month = jul,
	pages = {1--6},
	title = {On queueing in coded networks - queue size follows degrees of freedom},
	year = {2007},
	bdsk-url-1 = {https://doi.org/10.1109/ITWITWN.2007.4318066}}

