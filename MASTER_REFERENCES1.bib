	issn = {1558-0857},
	journal = {IEEE Transactions on Communications},
	keywords = {Adaptive modulation, Delay estimation, Demodulation, Fading, fading channels, Feedback, Gauss--Markov, Gaussian channels, Interleaved codes, Modulation coding, pilot symbol assisted modulation, Rayleigh fading, time-varying channels, Time-varying channels, Transceivers, Transmitters},
	month = jun,
	note = {Conference Name: IEEE Transactions on Communications},
	number = {6},
