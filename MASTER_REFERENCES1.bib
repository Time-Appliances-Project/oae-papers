	issn = {1872-4973},
	journal = {Forensic Science International: Genetics},
	keywords = {Distribution, G-test, Noise, Peak height, Short tandem repeat, Stutter},
	language = {en},
	month = nov,
	pages = {107--122},
	title = {Probabilistic characterisation of baseline noise in STR profiles},
