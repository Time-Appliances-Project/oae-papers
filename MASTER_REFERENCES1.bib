	keywords = {Physics - History and Philosophy of Physics, Quantum Physics},
	month = mar,
	note = {arXiv:1503.06413 [physics, physics:quant-ph]},
	publisher = {arXiv},
	title = {Causarum Investigatio and the Two {Bell}'s Theorems of John Bell},
	url = {http://arxiv.org/abs/1503.06413},
	urldate = {2023-01-03},
	year = {2015},
